	"description": "Portiny/Elasticsearch integration extension for Nette Framework.",
	"keywords": ["php", "nette", "portiny", "elasticsearch", "elastica"],
	"license": ["MIT"],
	"authors": [
		{ "name": "Tomáš Pilař", "email": "email@tomaspilar.cz", "homepage": "https://www.tomaspilar.cz/" }
	],
	"require": {
		"php": ">=7.1",
		"nette/di": "^3.0",
		"portiny/elasticsearch": "^4.0",
		"tracy/tracy": "^2.6"
	},
	"require-dev": {
		"nette/bootstrap": "^3.0",
		"phpunit/phpunit": "^8.3"
