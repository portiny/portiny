{
	"name": "portiny/console",
	"description": "Lightweight Symfony/Console integration extension for Nette framework.",
	"keywords": ["nette", "portiny", "symfony", "console"],
	"license": ["MIT"],
	"authors": [
		{ "name": "Tomáš Pilař", "email": "email@tomaspilar.cz", "homepage": "https://www.tomaspilar.cz/" }
	],
	"require": {
		"php": "~7.1",
		"nette/bootstrap": "^2.4",
		"nette/di": "^2.4",
		"nette/http": "^2.4",
		"symfony/console": "^4.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.5",
		"tracy/tracy": "^2.4"
	},
	"autoload": {
		"psr-4": {
			"Portiny\\Console\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Portiny\\Console\\Tests\\": "tests"
		}
	}
}
