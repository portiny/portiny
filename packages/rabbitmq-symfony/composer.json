{
	"name": "portiny/rabbitmq-symfony",
	"description": "Portiny/RabbitMQ integration extension for Symfony Framework.",
	"keywords": ["php", "symfony", "portiny", "rabbitmq", "bunny", "amqp"],
	"license": ["MIT"],
	"authors": [
		{ "name": "Tomáš Pilař", "email": "email@tomaspilar.cz", "homepage": "https://www.tomaspilar.cz/" }
	],
	"require": {
		"php": ">=7.1",
		"portiny/rabbitmq": "^4.0",
		"symfony/config": "^4.3",
		"symfony/dependency-injection": "^4.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.3"
	},
	"autoload": {
		"psr-4": {
			"Portiny\\RabbitMQSymfony\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Portiny\\RabbitMQSymfony\\Tests\\": "tests"
		}
	}
}
