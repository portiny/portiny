{
	"name": "portiny/rabbitmq-nette",
	"description": "Portiny/RabbitMQ integration extension for Nette Framework.",
	"keywords": ["php", "nette", "portiny", "rabbitmq", "bunny", "amqp"],
	"license": ["MIT"],
	"authors": [
		{ "name": "Tomáš Pilař", "email": "email@tomaspilar.cz", "homepage": "https://www.tomaspilar.cz/" }
	],
	"require": {
		"php": "^8.0",
		"nette/di": "^3.0",
		"portiny/rabbitmq": "^8.0"
	},
	"require-dev": {
		"nette/bootstrap": "^3.0",
		"phpunit/phpunit": "^7.5 || ^8.5 || ^9.3"
	},
	"autoload": {
		"psr-4": {
			"Portiny\\RabbitMQNette\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Portiny\\RabbitMQNette\\Tests\\": "tests"
		}
	}
}
